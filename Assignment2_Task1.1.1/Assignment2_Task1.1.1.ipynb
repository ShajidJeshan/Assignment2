    "def my_reduce(function,sequence):\n",
    "    leng=len(sequence)\n",
    "    para1=sequence[0]\n",
    "    for i in range(1,leng):\n",
    "        para2=sequence[i]\n",
    "        para1=function(para1,para2)\n",
    "    return para1\n",
    "\n",
    "\n",
    "def summ(x,y):\n",
    "    return x+y\n",
    "lis=[1,2,3,4,5]\n",
    "print(my_reduce(summ,lis))"
   ]
  }
